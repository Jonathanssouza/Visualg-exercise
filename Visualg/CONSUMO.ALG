Algoritmo "consumo"


Var

  distancia: inteiro
  combustivel, consumoMedio: real
  

Inicio

   escreva("Distancia percorrida: ")
   leia(distancia)
   escreva("Combustível gasto: ")
   leia(combustivel)
   escreval()
   
   consumoMedio <-  distancia/combustivel
   
   escreva("Consumo medio = ", consumoMedio:6:3)



Fimalgoritmo